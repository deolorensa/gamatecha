{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport { useAuthStore } from \"../stores/auth\";\nimport { VueGoodTable } from \"vue-good-table-next\";\nimport \"vue-good-table-next/dist/vue-good-table.css\";\nexport default {\n  components: {\n    VueGoodTable\n  },\n  setup() {\n    const authStore = useAuthStore();\n    const apiData = ref([]);\n    const columns = [{\n      label: \"Username\",\n      field: \"username\"\n    }, {\n      label: \"Email\",\n      field: \"email\"\n    }, {\n      label: \"Actions\",\n      sortable: false\n    }];\n    const fetchData = async () => {\n      try {\n        const response = await axios.get(\"http://103.164.54.252:8000/api/users\", {\n          headers: {\n            Authorization: `Bearer ${authStore.token}`\n          }\n        });\n        apiData.value = response.data.map(user => ({\n          username: user.username,\n          email: user.email,\n          id: user.id // Untuk digunakan dalam fungsi edit dan delete\n        }));\n        console.log(apiData.value); // Output data ke konsol untuk memeriksa strukturnya\n      } catch (error) {\n        console.error(\"Failed to fetch data:\", error);\n      }\n    };\n    const editUser = user => {\n      // Logika untuk mengedit pengguna\n      console.log(\"Edit user:\", user);\n    };\n    const deleteUser = async userId => {\n      try {\n        await axios.delete(`http://103.164.54.252:8000/api/users/${userId}`, {\n          headers: {\n            Authorization: `Bearer ${authStore.token}`\n          }\n        });\n        fetchData(); // Memuat ulang data setelah penghapusan berhasil\n      } catch (error) {\n        console.error(\"Failed to delete user:\", error);\n      }\n    };\n    onMounted(() => {\n      fetchData(); // Memuat data saat komponen dimuat\n    });\n    return {\n      columns,\n      apiData,\n      editUser,\n      deleteUser\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","useAuthStore","VueGoodTable","components","setup","authStore","apiData","columns","label","field","sortable","fetchData","response","get","headers","Authorization","token","value","data","map","user","username","email","id","console","log","error","editUser","deleteUser","userId","delete"],"sources":["/Users/deolorensa/vue/tech--gamatecha/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"container mx-auto p-6\">\n    <h1 class=\"text-2xl font-bold mb-4\">Home</h1>\n\n    <vue-good-table\n      :columns=\"columns\"\n      :rows=\"apiData\"\n      :paginate=\"true\"\n      :lineNumbers=\"true\"\n      :search=\"true\"\n    >\n      <template v-slot:table-row=\"props\">\n        <vg-cell>{{ props.row.username }}</vg-cell>\n        <vg-cell>{{ props.row.email }}</vg-cell>\n        <vg-cell>\n          <button\n            @click=\"editUser(props.row)\"\n            class=\"bg-blue-500 text-white px-2 py-1 rounded\"\n          >\n            Edit\n          </button>\n          <button\n            @click=\"deleteUser(props.row.id)\"\n            class=\"bg-red-500 text-white px-2 py-1 rounded ml-2\"\n          >\n            Delete\n          </button>\n        </vg-cell>\n      </template>\n    </vue-good-table>\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport { useAuthStore } from \"../stores/auth\";\nimport { VueGoodTable } from \"vue-good-table-next\";\nimport \"vue-good-table-next/dist/vue-good-table.css\";\n\nexport default {\n  components: {\n    VueGoodTable,\n  },\n  setup() {\n    const authStore = useAuthStore();\n    const apiData = ref([]);\n\n    const columns = [\n      { label: \"Username\", field: \"username\" },\n      { label: \"Email\", field: \"email\" },\n      { label: \"Actions\", sortable: false },\n    ];\n\n    const fetchData = async () => {\n      try {\n        const response = await axios.get(\n          \"http://103.164.54.252:8000/api/users\",\n          {\n            headers: {\n              Authorization: `Bearer ${authStore.token}`,\n            },\n          }\n        );\n\n        apiData.value = response.data.map((user) => ({\n          username: user.username,\n          email: user.email,\n          id: user.id, // Untuk digunakan dalam fungsi edit dan delete\n        }));\n\n        console.log(apiData.value); // Output data ke konsol untuk memeriksa strukturnya\n      } catch (error) {\n        console.error(\"Failed to fetch data:\", error);\n      }\n    };\n\n    const editUser = (user) => {\n      // Logika untuk mengedit pengguna\n      console.log(\"Edit user:\", user);\n    };\n\n    const deleteUser = async (userId) => {\n      try {\n        await axios.delete(`http://103.164.54.252:8000/api/users/${userId}`, {\n          headers: {\n            Authorization: `Bearer ${authStore.token}`,\n          },\n        });\n        fetchData(); // Memuat ulang data setelah penghapusan berhasil\n      } catch (error) {\n        console.error(\"Failed to delete user:\", error);\n      }\n    };\n\n    onMounted(() => {\n      fetchData(); // Memuat data saat komponen dimuat\n    });\n\n    return {\n      columns,\n      apiData,\n      editUser,\n      deleteUser,\n    };\n  },\n};\n</script>\n\n<style scoped>\n/* Gaya tambahan jika diperlukan */\n</style>\n"],"mappings":"AAkCA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,YAAW,QAAS,gBAAgB;AAC7C,SAASC,YAAW,QAAS,qBAAqB;AAClD,OAAO,6CAA6C;AAEpD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIJ,YAAY,CAAC,CAAC;IAChC,MAAMK,OAAM,GAAIR,GAAG,CAAC,EAAE,CAAC;IAEvB,MAAMS,OAAM,GAAI,CACd;MAAEC,KAAK,EAAE,UAAU;MAAEC,KAAK,EAAE;IAAW,CAAC,EACxC;MAAED,KAAK,EAAE,OAAO;MAAEC,KAAK,EAAE;IAAQ,CAAC,EAClC;MAAED,KAAK,EAAE,SAAS;MAAEE,QAAQ,EAAE;IAAM,CAAC,CACtC;IAED,MAAMC,SAAQ,GAAI,MAAAA,CAAA,KAAY;MAC5B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAC9B,sCAAsC,EACtC;UACEC,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUV,SAAS,CAAC