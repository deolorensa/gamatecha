{"ast":null,"code":"import { ref, onMounted, computed } from \"vue\";\nimport axios from \"axios\";\nimport { useAuthStore } from \"../stores/auth\";\nimport { VueGoodTable } from \"vue-good-table-next\";\nimport \"vue-good-table-next/dist/vue-good-table-next.css\";\nexport default {\n  components: {\n    VueGoodTable\n  },\n  setup() {\n    const authStore = useAuthStore();\n    const articles = ref([]);\n    const searchTerm = ref(\"\");\n    const filteredArticles = computed(() => {\n      return articles.value.filter(article => article.title.toLowerCase().includes(searchTerm.value.toLowerCase()));\n    });\n    const columns = [{\n      label: \"Image\",\n      field: \"image\",\n      type: \"image\"\n    }, {\n      label: \"Title\",\n      field: \"title\"\n    }, {\n      label: \"Source URL\",\n      field: \"source_url\"\n    }];\n    const paginationOptions = {\n      enabled: true,\n      perPage: 10\n    };\n    const fetchArticles = async () => {\n      try {\n        const response = await axios.get(\"http://103.164.54.252:8000/api/articles?limit=20\", {\n          headers: {\n            Authorization: `Bearer ${authStore.token}`\n          }\n        });\n        articles.value = response.data.data;\n      } catch (error) {\n        console.error(\"Failed to fetch articles:\", error);\n      }\n    };\n    onMounted(() => {\n      fetchArticles();\n    });\n    return {\n      columns,\n      articles,\n      paginationOptions,\n      searchTerm,\n      filteredArticles\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","axios","useAuthStore","VueGoodTable","components","setup","authStore","articles","searchTerm","filteredArticles","value","filter","article","title","toLowerCase","includes","columns","label","field","type","paginationOptions","enabled","perPage","fetchArticles","response","get","headers","Authorization","token","data","error","console"],"sources":["/Users/deolorensa/vue/tech--gamatecha/src/views/AdminArticles.vue"],"sourcesContent":["<template>\n  <div class=\"container mx-auto p-6\">\n    <h1 class=\"text-2xl font-bold mb-4\">Admin Articles</h1>\n    <div class=\"mb-4\">\n      <input\n        type=\"text\"\n        v-model=\"searchTerm\"\n        @input=\"handleSearch\"\n        placeholder=\"Search articles...\"\n        class=\"px-3 py-2 border border-gray-300 rounded-md w-full\"\n      />\n    </div>\n    <vue-good-table\n      :columns=\"columns\"\n      :rows=\"filteredArticles\"\n      :pagination-options=\"paginationOptions\"\n      :search-options=\"{\n        enabled: false,\n      }\"\n    >\n      <template v-slot:table-row=\"props\">\n        <span v-if=\"props.column.field === 'image'\">\n          <img\n            :src=\"props.row.image\"\n            alt=\"Article Image\"\n            class=\"w-16 h-16 object-cover rounded\"\n          />\n        </span>\n        <span v-else-if=\"props.column.field === 'source_url'\">\n          <a\n            :href=\"props.row.source_url\"\n            target=\"_blank\"\n            class=\"text-blue-500 underline\"\n          >\n            {{ props.row.source_url }}\n          </a>\n        </span>\n        <span v-else>\n          {{ props.formattedRow[props.column.field] }}\n        </span>\n      </template>\n    </vue-good-table>\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted, computed } from \"vue\";\nimport axios from \"axios\";\nimport { useAuthStore } from \"../stores/auth\";\nimport { VueGoodTable } from \"vue-good-table-next\";\nimport \"vue-good-table-next/dist/vue-good-table-next.css\";\n\nexport default {\n  components: {\n    VueGoodTable,\n  },\n  setup() {\n    const authStore = useAuthStore();\n    const articles = ref([]);\n    const searchTerm = ref(\"\");\n    const filteredArticles = computed(() => {\n      return articles.value.filter((article) =>\n        article.title.toLowerCase().includes(searchTerm.value.toLowerCase())\n      );\n    });\n\n    const columns = [\n      { label: \"Image\", field: \"image\", type: \"image\" },\n      { label: \"Title\", field: \"title\" },\n      { label: \"Source URL\", field: \"source_url\" },\n    ];\n\n    const paginationOptions = {\n      enabled: true,\n      perPage: 10,\n    };\n\n    const fetchArticles = async () => {\n      try {\n        const response = await axios.get(\n          \"http://103.164.54.252:8000/api/articles?limit=20\",\n          {\n            headers: {\n              Authorization: `Bearer ${authStore.token}`,\n            },\n          }\n        );\n        articles.value = response.data.data;\n      } catch (error) {\n        console.error(\"Failed to fetch articles:\", error);\n      }\n    };\n\n    onMounted(() => {\n      fetchArticles();\n    });\n\n    return {\n      columns,\n      articles,\n      paginationOptions,\n      searchTerm,\n      filteredArticles,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.container {\n  max-width: 1200px;\n  margin: 0 auto;\n}\n</style>\n"],"mappings":"AA8CA,SAASA,GAAG,EAAEC,SAAS,EAAEC,QAAO,QAAS,KAAK;AAC9C,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,YAAW,QAAS,gBAAgB;AAC7C,SAASC,YAAW,QAAS,qBAAqB;AAClD,OAAO,kDAAkD;AAEzD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIJ,YAAY,CAAC,CAAC;IAChC,MAAMK,QAAO,GAAIT,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMU,UAAS,GAAIV,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMW,gBAAe,GAAIT,QAAQ,CAAC,MAAM;MACtC,OAAOO,QAAQ,CAACG,KAAK,CAACC,MAAM,CAAEC,OAAO,IACnCA,OAAO,CAACC,KAAK,CAACC,WAAW,CAAC,CAAC,CAACC,QAAQ,CAACP,UAAU,CAACE,KAAK,CAACI,WAAW,CAAC,CAAC,CACrE,CAAC;IACH,CAAC,CAAC;IAEF,MAAME,OAAM,GAAI,CACd;MAAEC,KAAK,EAAE,OAAO;MAAEC,KAAK,EAAE,OAAO;MAAEC,IAAI,EAAE;IAAQ,CAAC,EACjD;MAAEF,KAAK,EAAE,OAAO;MAAEC,KAAK,EAAE;IAAQ,CAAC,EAClC;MAAED,KAAK,EAAE,YAAY;MAAEC,KAAK,EAAE;IAAa,CAAC,CAC7C;IAED,MAAME,iBAAgB,GAAI;MACxBC,OAAO,EAAE,IAAI;MACbC,OAAO,EAAE;IACX,CAAC;IAED,MAAMC,aAAY,GAAI,MAAAA,CAAA,KAAY;MAChC,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMvB,KAAK,CAACwB,GAAG,CAC9B,kDAAkD,EAClD;UACEC,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUrB,SAAS,CAACs