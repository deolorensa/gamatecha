{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport { useAuthStore } from \"../stores/auth\";\nimport { V3Datatable, V3DatatableColumn } from \"vue3-datatable\";\nexport default {\n  components: {\n    V3Datatable,\n    V3DatatableColumn\n  },\n  setup() {\n    const authStore = useAuthStore();\n    const apiData = ref([]);\n    const fetchData = async () => {\n      try {\n        const response = await axios.get(\"http://103.164.54.252:8000/api/users\", {\n          headers: {\n            Authorization: `Bearer ${authStore.token}`\n          }\n        });\n\n        // Mengubah struktur data dari array ke array objek yang sesuai dengan format DataTable\n        apiData.value = response.data.map(user => ({\n          username: user.username,\n          email: user.email,\n          actions: {\n            id: user.id\n            // Anda dapat menambahkan properti lain yang diperlukan untuk aksi lainnya\n          }\n        }));\n        console.log(apiData.value); // Output data ke konsol untuk memeriksa strukturnya\n      } catch (error) {\n        console.error(\"Failed to fetch data:\", error);\n      }\n    };\n    onMounted(() => {\n      fetchData();\n    });\n    return {\n      apiData\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","useAuthStore","V3Datatable","V3DatatableColumn","components","setup","authStore","apiData","fetchData","response","get","headers","Authorization","token","value","data","map","user","username","email","actions","id","console","log","error"],"sources":["/Users/deolorensa/vue/tech--gamatecha/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"container mx-auto p-6\">\n    <h1 class=\"text-2xl font-bold mb-4\">Home</h1>\n    <v3-datatable :data=\"apiData\">\n      <v3-datatable-column field=\"username\" label=\"Username\" />\n      <v3-datatable-column field=\"email\" label=\"Email\" />\n      <v3-datatable-column label=\"Actions\">\n        <template #default=\"{ row }\">\n          <button\n            @click=\"editUser(row)\"\n            class=\"bg-blue-500 text-white px-2 py-1 rounded\"\n          >\n            Edit\n          </button>\n          <button\n            @click=\"deleteUser(row.id)\"\n            class=\"bg-red-500 text-white px-2 py-1 rounded ml-2\"\n          >\n            Delete\n          </button>\n        </template>\n      </v3-datatable-column>\n    </v3-datatable>\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport { useAuthStore } from \"../stores/auth\";\nimport { V3Datatable, V3DatatableColumn } from \"vue3-datatable\";\n\nexport default {\n  components: {\n    V3Datatable,\n    V3DatatableColumn,\n  },\n  setup() {\n    const authStore = useAuthStore();\n    const apiData = ref([]);\n\n    const fetchData = async () => {\n      try {\n        const response = await axios.get(\n          \"http://103.164.54.252:8000/api/users\",\n          {\n            headers: {\n              Authorization: `Bearer ${authStore.token}`,\n            },\n          }\n        );\n\n        // Mengubah struktur data dari array ke array objek yang sesuai dengan format DataTable\n        apiData.value = response.data.map((user) => ({\n          username: user.username,\n          email: user.email,\n          actions: {\n            id: user.id,\n            // Anda dapat menambahkan properti lain yang diperlukan untuk aksi lainnya\n          },\n        }));\n\n        console.log(apiData.value); // Output data ke konsol untuk memeriksa strukturnya\n      } catch (error) {\n        console.error(\"Failed to fetch data:\", error);\n      }\n    };\n\n    onMounted(() => {\n      fetchData();\n    });\n\n    return {\n      apiData,\n    };\n  },\n};\n</script>\n\n<style scoped>\n/* Tambahkan style tambahan jika diperlukan */\n</style>\n"],"mappings":"AA2BA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC