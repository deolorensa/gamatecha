 {\n      enabled: true,\n      perPage: 10,\n    };\n\n    const fetchData = async () => {\n      try {\n        const response = await axios.get(\n          \"http://103.164.54.252:8000/api/articles\",\n          {\n            headers: {\n              Authorization: `Bearer ${authStore.token}`,\n            },\n          }\n        );\n\n        // Mengubah struktur data dari array ke array objek yang sesuai dengan format DataTable\n        apiData.value = response.data.map((user) => ({\n          username: user.id,\n          email: user.title,\n          actions: {\n            id: user.id,\n            // Anda dapat menambahkan properti lain yang diperlukan untuk aksi lainnya\n          },\n        }));\n\n        console.log(apiData.value); // Output data ke konsol untuk memeriksa strukturnya\n      } catch (error) {\n        console.error(\"Failed to fetch data:\", error);\n      }\n    };\n\n    const editUser = (user) => {\n      // Implementasi logika untuk mengedit pengguna\n      console.log(\"Edit user:\", user);\n    };\n\n    const deleteUser = async (id) => {\n      try {\n        await axios.delete(`http://103.164.54.252:8000/api/users/${id}`, {\n          headers: {\n            Authorization: `Bearer ${authStore.token}`,\n          },\n        });\n        fetchData();\n      } catch (error) {\n        console.error(\"Failed to delete user:\", error);\n      }\n    };\n\n    onMounted(() => {\n      fetchData();\n    });\n\n    return {\n      columns,\n      apiData,\n      paginationOptions,\n      editUser,\n      deleteUser,\n    };\n  },\n};\n</script>\n\n<style scoped>\n/* Tambahkan style tambahan jika diperlukan */\n</style>\n"],"mappings":";;;EACOA,KAAK,EAAC;AAAuB;gEAChCC,mBAAA,CAA8C;EAA1CD,KAAK,EAAC;AAAyB,GAAC,OAAK;;EAF7CE,GAAA;AAAA;mBAAA;mBAAA;;EAAAA,GAAA;AAAA;;;uBACEC,mBAAA,CA2BM,OA3BNC,UA2BM,GA1BJC,UAA8C,EAC9CC,YAAA,CAwBiBC,yBAAA;IAvBdC,OAAO,EAAEC,MAAA,CAAAD,OAAO;IAChBE,IAAI,EAAED,MAAA,CAAAE,OAAO;IACb,oBAAkB,EAAEF,MAAA,CAAAG;;IAEJ,WA