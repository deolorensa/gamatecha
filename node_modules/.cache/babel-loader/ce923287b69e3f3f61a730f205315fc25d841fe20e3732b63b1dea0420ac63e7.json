200\"\n          >\n            Login\n          </button>\n        </div>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref } from \"vue\";\nimport { useAuthStore } from \"../stores/auth\";\n\nexport default {\n  setup() {\n    const email = ref(\"\");\n    const password = ref(\"\");\n    const authStore = useAuthStore();\n\n    const login = async () => {\n      try {\n        await authStore.login(email.value, password.value);\n        alert(\"Login successful!\");\n        // Redirect or perform other actions after successful login\n      } catch (error) {\n        alert(\"Login failed. Please check your credentials and try again.\");\n      }\n    };\n\n    return {\n      email,\n      password,\n      login,\n    };\n  },\n};\n</script>\n\n<style scoped>\n/* Tambahkan style tambahan jika diperlukan */\n</style>\n"],"mappings":"AAmCA,SAASA,GAAE,QAAS,KAAK;AACzB,SAASC,YAAW,QAAS,gBAAgB;AAE7C,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,GAAG,CAAC,EAAE,CAAC;IACrB,MAAMI,QAAO,GAAIJ,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMK,SAAQ,GAAIJ,YAAY,CAAC,CAAC;IAEhC,MAAMK,KAAI,GAAI,MAAAA,CAAA,KAAY;MACxB,IAAI;QACF,MAAMD,SAAS,CAACC,KAAK,CAACH,KAAK,CAACI,KAAK,EAAEH,QAAQ,CAACG,KAAK,CAAC;QAClDC,KAAK,CAAC,mBAAmB,CAAC;QAC1B;MACF,EAAE,OAAOC,KAAK,EAAE;QACdD,KAAK,CAAC,4DAA4D,CAAC;MACrE;IACF,CAAC;IAED,OAAO;MACLL,KAAK;MACLC,QAAQ;MACRE;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}