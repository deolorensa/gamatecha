{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport { useAuthStore } from \"../stores/auth\";\nimport { VueGoodTable } from \"vue-good-table-next\";\nimport \"vue-good-table-next/dist/vue-good-table-next.css\";\nexport default {\n  components: {\n    VueGoodTable\n  },\n  setup() {\n    const authStore = useAuthStore();\n    const apiData = ref([]);\n    const showEditModal = ref(false);\n    const editUserForm = ref({\n      id: \"\",\n      name: \"\",\n      email: \"\"\n    });\n    const columns = [{\n      label: \"Id\",\n      field: \"id\"\n    }, {\n      label: \"Name\",\n      field: \"firstname\"\n    }, {\n      label: \"Email\",\n      field: \"email\"\n    }, {\n      label: \"Actions\",\n      field: \"actions\",\n      sortable: false\n    }];\n    const paginationOptions = {\n      enabled: true,\n      perPage: 10\n    };\n    const fetchData = async () => {\n      try {\n        const response = await axios.get(\"http://103.164.54.252:8000/api/users\", {\n          headers: {\n            Authorization: `Bearer ${authStore.token}`\n          }\n        });\n        apiData.value = response.data.map(user => ({\n          id: user.id,\n          firstname: user.first_name,\n          email: user.email\n        }));\n      } catch (error) {\n        console.error(\"Failed to fetch data:\", error);\n      }\n    };\n    const openEditModal = user => {\n      editUserForm.value = {\n        id: user.id,\n        name: user.firstname,\n        email: user.email\n      };\n      showEditModal.value = true;\n    };\n    const updateUser = async () => {\n      try {\n        await axios.put(`http://103.164.54.252:8000/api/users/${editUserForm.value.id}`, {\n          first_name: editUserForm.value.name,\n          email: editUserForm.value.email\n        }, {\n          headers: {\n            Authorization: `Bearer ${authStore.token}`\n          }\n        });\n        fetchData();\n        showEditModal.value = false;\n      } catch (error) {\n        console.error(\"Failed to update user:\", error);\n      }\n    };\n    const deleteUser = async id => {\n      try {\n        await axios.delete(`http://103.164.54.252:8000/api/users/${id}`, {\n          headers: {\n            Authorization: `Bearer ${authStore.token}`\n          }\n        });\n        fetchData();\n      } catch (error) {\n        console.error(\"Failed to delete user:\", error);\n      }\n    };\n    onMounted(() => {\n      fetchData();\n    });\n    return {\n      columns,\n      apiData,\n      paginationOptions,\n      showEditModal,\n      editUserForm,\n      openEditModal,\n      updateUser,\n      deleteUser\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","useAuthStore","VueGoodTable","components","setup","authStore","apiData","showEditModal","editUserForm","id","name","email","columns","label","field","sortable","paginationOptions","enabled","perPage","fetchData","response","get","headers","Authorization","token","value","data","map","user","firstname","first_name","error","console","openEditModal","updateUser","put","deleteUser","delete"],"sources":["/Users/deolorensa/vue/tech--gamatecha/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"container mx-auto p-6\">\n    <h1 class=\"text-2xl font-bold mb-4\">Users</h1>\n    <vue-good-table\n      :columns=\"columns\"\n      :rows=\"apiData\"\n      :pagination-options=\"paginationOptions\"\n    >\n      <template v-slot:table-row=\"props\">\n        <span v-if=\"props.column.field === 'actions'\">\n          <button\n            @click=\"openEditModal(props.row)\"\n            class=\"bg-blue-500 text-white px-2 py-1 rounded\"\n          >\n            Edit\n          </button>\n          <button\n            @click=\"deleteUser(props.row.id)\"\n            class=\"bg-red-500 text-white px-2 py-1 rounded ml-2\"\n          >\n            Delete\n          </button>\n        </span>\n        <span v-else>\n          {{ props.formattedRow[props.column.field] }}\n        </span>\n      </template>\n    </vue-good-table>\n\n    <!-- Modal Edit User -->\n    <div\n      v-if=\"showEditModal\"\n      class=\"fixed inset-0 flex items-center justify-center bg-black bg-opacity-50\"\n    >\n      <div class=\"bg-white p-6 rounded shadow-lg w-1/3\">\n        <h2 class=\"text-xl font-bold mb-4\">Edit User</h2>\n        <form @submit.prevent=\"updateUser\">\n          <div class=\"mb-4\">\n            <label class=\"block text-gray-700\">Name</label>\n            <input\n              type=\"text\"\n              v-model=\"editUserForm.name\"\n              class=\"w-full px-3 py-2 border rounded\"\n            />\n          </div>\n          <div class=\"mb-4\">\n            <label class=\"block text-gray-700\">Email</label>\n            <input\n              type=\"email\"\n              v-model=\"editUserForm.email\"\n              class=\"w-full px-3 py-2 border rounded\"\n            />\n          </div>\n          <div class=\"flex justify-end\">\n            <button\n              type=\"button\"\n              @click=\"showEditModal = false\"\n              class=\"bg-gray-500 text-white px-3 py-2 rounded mr-2\"\n            >\n              Cancel\n            </button>\n            <button\n              type=\"submit\"\n              class=\"bg-blue-500 text-white px-3 py-2 rounded\"\n            >\n              Save\n            </button>\n          </div>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport { useAuthStore } from \"../stores/auth\";\nimport { VueGoodTable } from \"vue-good-table-next\";\nimport \"vue-good-table-next/dist/vue-good-table-next.css\";\n\nexport default {\n  components: {\n    VueGoodTable,\n  },\n  setup() {\n    const authStore = useAuthStore();\n    const apiData = ref([]);\n    const showEditModal = ref(false);\n    const editUserForm = ref({ id: \"\", name: \"\", email: \"\" });\n\n    const columns = [\n      { label: \"Id\", field: \"id\" },\n      { label: \"Name\", field: \"firstname\" },\n      { label: \"Email\", field: \"email\" },\n      { label: \"Actions\", field: \"actions\", sortable: false },\n    ];\n\n    const paginationOptions = {\n      enabled: true,\n      perPage: 10,\n    };\n\n    const fetchData = async () => {\n      try {\n        const response = await axios.get(\n          \"http://103.164.54.252:8000/api/users\",\n          {\n            headers: {\n              Authorization: `Bearer ${authStore.token}`,\n            },\n          }\n        );\n\n        apiData.value = response.data.map((user) => ({\n          id: user.id,\n          firstname: user.first_name,\n          email: user.email,\n        }));\n      } catch (error) {\n        console.error(\"Failed to fetch data:\", error);\n      }\n    };\n\n    const openEditModal = (user) => {\n      editUserForm.value = {\n        id: user.id,\n        name: user.firstname,\n        email: user.email,\n      };\n      showEditModal.value = true;\n    };\n\n    const updateUser = async () => {\n      try {\n        await axios.put(\n          `http://103.164.54.252:8000/api/users/${editUserForm.value.id}`,\n          {\n            first_name: editUserForm.value.name,\n            email: editUserForm.value.email,\n          },\n          {\n            headers: {\n              Authorization: `Bearer ${authStore.token}`,\n            },\n          }\n        );\n        fetchData();\n        showEditModal.value = false;\n      } catch (error) {\n        console.error(\"Failed to update user:\", error);\n      }\n    };\n\n    const deleteUser = async (id) => {\n      try {\n        await axios.delete(`http://103.164.54.252:8000/api/users/${id}`, {\n          headers: {\n            Authorization: `Bearer ${authStore.token}`,\n          },\n        });\n        fetchData();\n      } catch (error) {\n        console.error(\"Failed to delete user:\", error);\n      }\n    };\n\n    onMounted(() => {\n      fetchData();\n    });\n\n    return {\n      columns,\n      apiData,\n      paginationOptions,\n      showEditModal,\n      editUserForm,\n      openEditModal,\n      updateUser,\n      deleteUser,\n    };\n  },\n};\n</script>\n\n<style scoped>\n/* Tambahkan style tambahan jika diperlukan */\n</style>\n"],"mappings":"AA2EA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,YAAW,QAAS,gBAAgB;AAC7C,SAASC,YAAW,QAAS,qBAAqB;AAClD,OAAO,kDAAkD;AAEzD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIJ,YAAY,CAAC,CAAC;IAChC,MAAMK,OAAM,GAAIR,GAAG,CAAC,EAAE,CAAC;IACvB,MAAMS,aAAY,GAAIT,GAAG,CAAC,KAAK,CAAC;IAChC,MAAMU,YAAW,GAAIV,GAAG,CAAC;MAAEW,EAAE,EAAE,EAAE;MAAEC,IAAI,EAAE,EAAE;MAAEC,KAAK,EAAE;IAAG,CAAC,CAAC;IAEzD,MAAMC,OAAM,GAAI,CACd;MAAEC,KAAK,EAAE,IAAI;MAAEC,KAAK,EAAE;IAAK,CAAC,EAC5B;MAAED,KAAK,EAAE,MAAM;MAAEC,KAAK,EAAE;IAAY,CAAC,EACrC;MAAED,KAAK,EAAE,OAAO;MAAEC,KAAK,EAAE;IAAQ,CAAC,EAClC;MAAED,KAAK,EAAE,SAAS;MAAEC,KAAK,EAAE,SAAS;MAAEC,QAAQ,EAAE;IAAM,CAAC,CACxD;IAED,MAAMC,iBAAgB,GAAI;MACxBC,OAAO,EAAE,IAAI;MACbC,OAAO,EAAE;IACX,CAAC;IAED,MAAMC,SAAQ,GAAI,MAAAA,CAAA,KAAY;MAC5B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMpB,KAAK,CAACqB,GAAG,CAC9B,sCAAsC,EACtC;UACEC,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUlB,SAAS,CAACmB,KAAK;UAC1C;QACF,CACF,CAAC;QAEDlB,OAAO,CAACmB,KAAI,GAAIL,QAAQ,CAACM,IAAI,CAACC,GAAG,CAAEC,IAAI,KAAM;UAC3CnB,EAAE,EAAEmB,IAAI,CAACnB,EAAE;UACXoB,SAAS,EAAED,IAAI,CAACE,UAAU;UAC1BnB,KAAK,EAAEiB,IAAI,CAACjB;QACd,CAAC,CAAC,CAAC;MACL,EAAE,OAAOoB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C;IACF,CAAC;IAED,MAAME,aAAY,GAAKL,IAAI,IAAK;MAC9BpB,YAAY,CAACiB,KAAI,GAAI;QACnBhB,EAAE,EAAEmB,IAAI,CAACnB,EAAE;QACXC,IAAI,EAAEkB,IAAI,CAACC,SAAS;QACpBlB,KAAK,EAAEiB,IAAI,CAACjB;MACd,CAAC;MACDJ,aAAa,CAACkB,KAAI,GAAI,IAAI;IAC5B,CAAC;IAED,MAAMS,UAAS,GAAI,MAAAA,CAAA,KAAY;MAC7B,IAAI;QACF,MAAMlC,KAAK,CAACmC,GAAG,CACb,wCAAwC3B,YAAY,CAACiB,KAAK,CAAChB,E